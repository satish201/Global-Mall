<div style="height: 894px; overflow: scroll;">

<%= form_for(@mallmanager, :validate => true, :url => mallmanagers_path + ".js", :html => {:multipart => true, :target => 'frame'} ) do |f| %>
  <% if @mallmanager.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@mallmanager.errors.count, "error") %> prohibited this mallmanager from being saved:</h2>

      <ul>
      <% @mallmanager.errors.full_messages.each do |msg| %>
